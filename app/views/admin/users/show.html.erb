<center>
  <div class="jumbotron">
    <%= image_tag profile_img, class: 'profile-img' %>
    <br>
    <section id="user-info-<%=@user.id %>">
      <p class="lead">Name: <%= @user.name %></p>
      <p class="lead">Address: <%= @user.default_address %></p>
      <p class="lead">Email: <%= @user.email %></p>
    </section>
  </div>
</center>
